{
  "name": "react-portfolio",
  "version": "1.0.0",
  "description": "My professional portfolio describing my experience as a developer.",
  "engines": {
    "node": "10.15.3"
  },
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NFRNO94/ReactPortfolio.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NFRNO94/ReactPortfolio/issues"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "helmet": "^3.20.0",
    "if-env": "^1.0.4",
    "install": "^0.13.0",
    "mongoose": "^5.6.9",
    "react-router-dom": "^5.0.1",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "nodemon": "^1.18.7"
  },
  "homepage": "https://github.com/NFRNO94/ReactPortfolio#readme"
}
